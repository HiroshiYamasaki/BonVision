      <FramebufferAttachments />
      <VertexShader>Shaders\Quad.vert</VertexShader>
      <FragmentShader>Shaders\Color.frag</FragmentShader>
    </ShaderConfiguration>
    <ShaderConfiguration xsi:type="Material">
      <Name>Circle</Name>
      <RenderState />
      <ShaderUniforms />
