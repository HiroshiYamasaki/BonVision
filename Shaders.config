      <VertexShader>Shaders\Quad.vert</VertexShader>
      <FragmentShader>Shaders\Circle.frag</FragmentShader>
    </ShaderConfiguration>
    <ShaderConfiguration xsi:type="Material">
      <Name>Gamma</Name>
      <RenderState />
      <ShaderUniforms />
