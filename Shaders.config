<?xml version="1.0" encoding="utf-8"?>
<ShaderWindowSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="clr-namespace:Bonsai.Shaders;assembly=Bonsai.Shaders">
  <Width>640</Width>
  <Height>480</Height>
  <VSync>On</VSync>
  <SwapSync>false</SwapSync>
  <ClearColor>Gray</ClearColor>
  <ClearMask>DepthBufferBit ColorBufferBit</ClearMask>
  <CursorVisible>true</CursorVisible>
  <Location xsi:nil="true" />
  <WindowBorder>Resizable</WindowBorder>
  <WindowState>Normal</WindowState>
  <DisplayDevice>Primary</DisplayDevice>
  <TargetRenderFrequency>0</TargetRenderFrequency>
